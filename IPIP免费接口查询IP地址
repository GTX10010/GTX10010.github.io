#利用ipip.net免费接口查询IP地址
import requests
from time import sleep
url = "http://freeapi.ipip.net/"
try:
     ip = input("请输入IP地址")
     r = requests.get(url + ip)
     r.raise_for_status()
     r.encoding = r.apparent_encoding
     print(r.text)
except:
     print("查询失败")
print("15s后退出程序")
sleep(15)
